{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "framework": "nextjs",
  "buildCommand": "pnpm prisma generate && pnpm build",
  "installCommand": "pnpm install",
  "regions": ["iad1"],
  "functions": {
    "src/app/api/search/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/**/*": {
      "maxDuration": 60
    }
  }
}
